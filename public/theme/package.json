{
  "name": "siliconile-theme",
  "version": "1.0.0",
  "description": "Static HTML theme for Siliconile website",
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/app.css -o ./assets/css/styles.css --minify",
    "watch:css": "npx tailwindcss -i ./src/app.css -o ./assets/css/styles.css --watch"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.0",
    "tailwindcss": "^4.0.0"
  }
}
